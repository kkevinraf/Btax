{% extends '::layout_admin_cooperative.html.twig' %}

{% block body -%}
    <h1>Place</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Typeplace</th>
                <td>{{ entity.typePlace }}</td>
            </tr>
            <tr>
                <th>Couleur</th>
                <td>{{ entity.couleur }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('place') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('place_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
